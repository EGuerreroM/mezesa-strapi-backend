{
  "kind": "collectionType",
  "collectionName": "product_colors",
  "info": {
    "singularName": "product-color",
    "pluralName": "product-colors",
    "displayName": "ProductColor"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "product_colors"
    },
    "color": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::color.color",
      "inversedBy": "product_colors"
    },
    "product_images": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-image.product-image",
      "mappedBy": "product_color"
    },
    "displayOrder": {
      "type": "integer",
      "default": 1
    },
    "isAvailable": {
      "type": "boolean",
      "default": true
    }
  }
}
